<Window x:Class="EnterpriseWPF.TreeViewDemo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TreeViewDemo" Height="300" Width="300">
    <Grid>
        <TreeView>
            <TreeViewItem Header="Humans"></TreeViewItem>
            <TreeViewItem Header="Animals">
                <TreeViewItem Header="Mammals">
                    <TreeViewItem Header="Dogs">
                        <TreeViewItem Header="Spaniels" />
                        <TreeViewItem Header="Working Dogs" />
                        <TreeViewItem Header="Retrievers" />
                    </TreeViewItem>
                    <TreeViewItem Header="Cats">
                        <TreeViewItem Header="Tiger" />
                        <TreeViewItem Header="Leopard" />
                        <TreeViewItem Header="Cougar" />
                    </TreeViewItem>
                </TreeViewItem>
                <TreeViewItem Header="Marsupials">
                    <TreeViewItem Header="kangaroo" />
                    <TreeViewItem Header="Koala" />
                </TreeViewItem>
            </TreeViewItem>
            <TreeViewItem Header="Plants"></TreeViewItem>
            <TreeViewItem Header="Others"></TreeViewItem>
        </TreeView>
    </Grid>
</Window>
